@model IEnumerable<StackUndertow.Models.Question>

@{
    ViewBag.Title = "All Questions";
}

<h2>Index</h2>

<div class="row">
    <div class="col-md-6">
        @foreach (var question in Model)
        {
           <h1>question.Title</h1>
           <hr />
           <h3>question.User.Name asked: </h3>
           <p>question.Body</p>           
        }
    </div>
</div>